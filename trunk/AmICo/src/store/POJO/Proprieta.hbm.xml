<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="store.POJO">

	<class name="Proprieta" table="PROPRIETA">
	
		<id name="id" column="PROPRIETA_ID">
			<generator class="native"/>
		</id>
		<property name="quota"/>
		
		<set name="proprietario" table="PROPRIETA_PERSONE" cascade="save-update">
 			<key column="PROPRIETA_ID"/>
 			<many-to-many column="PERSONA_ID" class="Persona"/>
		</set>
	</class>

</hibernate-mapping>
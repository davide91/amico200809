<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="store.POJO">

	<class name="Condominio" table="CONDOMINI">
	
		<id name="id" column="CONDOMINI_ID">
			<generator class="native"/>
		</id>
		
		
		
		
		<set name="unitaImmobiliari" cascade="all-delete-orphan,save-update">
  			<key column="CONDOMINIO_ID"/>
  			<one-to-many class="UnitaImmobiliare"/>
		</set>
		
		
		
		<component name="NomeDataType" class="Classe">
 			<property name="NomeCampo1"/>
 			...
		 	<property name="NomeCampoN"/>
		</component>

	
	
	

	</class>

</hibernate-mapping>
